<?xml version="1.0" encoding="UTF-8"?><window>    <controls>        <!-- DialogBackground -->        <control type="image">            <left>-2000</left>            <top>-2000</top>            <width>6000</width>            <height>6000</height>            <animation effect="fade" time="300">VisibleChange</animation>            <animation effect="fade" start="0" end="100" time="300">WindowOpen</animation>            <animation effect="fade" start="100" end="0" time="200">WindowClose</animation>            <texture colordiffuse="80FFFFFF">colors/black.png</texture>        </control>        <!-- DialogWindow-->        <control type="group">            <centerleft>50%</centerleft>            <centertop>50%</centertop>            <width>1400</width>            <height>850</height>            <!-- Background -->            <control type="group">                <width>1400</width>                <height>800</height>                <control type="image">                    <texture colordiffuse="FF1A2123">colors/white.png</texture>                </control>                <control type="image">                    <height>70</height>                    <texture colordiffuse="FF12A0C7">colors/white70.png</texture>                </control>                <control type="label">                    <left>40</left>                    <right>100</right>                    <height>70</height>                    <font>font32_title</font>                    <label>$ADDON[plugin.video.duboku 30129]</label>                    <aligny>center</aligny>                    <shadowcolor>black</shadowcolor>                </control>            </control>            <!-- Navigation -->            <control type="list" id="1100">                <left>40</left>                <top>110</top>                <width>300</width>                <height>650</height>                <onright>1101</onright>                <itemlayout height="90">                    <control type="button">                        <width>300</width>                        <height>80</height>                        <texturefocus colordiffuse="FF12A0C7">lists/focus.png</texturefocus>                        <texturenofocus colordiffuse="99FFFFFF">lists/panel.png</texturenofocus>                    </control>                    <control type="label">                        <width>300</width>                        <height>80</height>                        <align>center</align>                        <aligny>center</aligny>                        <label>$INFO[ListItem.Label]</label>                        <font>font25_title</font>                    </control>                </itemlayout>                <focusedlayout height="90">                    <control type="button">                        <width>300</width>                        <height>80</height>                        <texturefocus colordiffuse="FF12A0C7">lists/focus.png</texturefocus>                        <texturenofocus colordiffuse="99FFFFFF">lists/panel.png</texturenofocus>                        <onfocus>SetFocus(1100)</onfocus>                    </control>                    <control type="label">                        <width>300</width>                        <height>80</height>                        <align>center</align>                        <aligny>center</aligny>                        <label>$INFO[ListItem.Label]</label>                        <font>font25_title</font>                    </control>                </focusedlayout>            </control>            <!-- Content -->            <control type="image">                <left>370</left>                <top>110</top>                <width>690</width>                <height>650</height>                <texture colordiffuse="99FFFFFF">lists/panel.png</texture>            </control>            <control type="list" id="1101">                <left>370</left>                <top>110</top>                <width>690</width>                <height>650</height>                <onleft>1100</onleft>                <onright>1102</onright>                <pagecontrol>1102</pagecontrol>                <itemlayout height="75">                    <control type="button">                        <width>690</width>                        <height>70</height>                        <texturefocus colordiffuse="FF12A0C7">lists/focus.png</texturefocus>                        <texturenofocus/>                    </control>                    <control type="label">                        <width>690</width>                        <textoffsetx>40</textoffsetx>                        <height>70</height>                        <aligny>center</aligny>                        <label>$INFO[ListItem.Label]</label>                    </control>                </itemlayout>                <focusedlayout height="75">                    <control type="button">                        <width>690</width>                        <height>70</height>                        <texturefocus colordiffuse="FF12A0C7">lists/focus.png</texturefocus>                        <texturenofocus/>                    </control>                    <control type="label">                        <textoffsetx>40</textoffsetx>                        <width>690</width>                        <height>70</height>                        <aligny>center</aligny>                        <label>$INFO[ListItem.Label]</label>                    </control>                </focusedlayout>            </control>            <control type="scrollbar" id="1102">                <left>1065</left>                <top>110</top>                <width>12</width>                <height>650</height>                <onleft>1101</onleft>                <onright>1103</onright>                <orientation>vertical</orientation>            </control>            <!-- Sidebar -->            <control type="grouplist" id="1103">                <left>1080</left>                <top>90</top>                <onleft>1101</onleft>                <control type="button" id="1104">                    <width>300</width>                    <height>100</height>                    <label>$LOCALIZE[186]</label>                    <font>font25_title</font>                    <textoffsetx>20</textoffsetx>                    <align>center</align>                    <texturefocus border="40" colordiffuse="FF12A0C7">buttons/dialogbutton-fo.png</texturefocus>                    <texturenofocus border="40">buttons/dialogbutton-nofo.png</texturenofocus>                </control>                <control type="button" id="1105">                    <width>300</width>                    <height>100</height>                    <label>$LOCALIZE[222]</label>                    <font>font25_title</font>                    <textoffsetx>20</textoffsetx>                    <onclick>Action(close)</onclick>                    <align>center</align>                    <texturefocus border="40" colordiffuse="FF12A0C7">buttons/dialogbutton-fo.png</texturefocus>                    <texturenofocus border="40">buttons/dialogbutton-nofo.png</texturenofocus>                </control>            </control>        </control>    </controls></window>